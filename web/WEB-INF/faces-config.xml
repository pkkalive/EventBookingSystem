<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>/user/userLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>eventmanager</from-outcome>
            <to-view-id>/eventManager/eventManagerLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin/adminLogin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/userLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginOK</from-outcome>
            <to-view-id>/user/festivalsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id>/user/festivalsSummary.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bookTickets</from-outcome>
            <to-view-id>/user/createBooking.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>myTransactions</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userEventDetails</from-outcome>
            <to-view-id>/user/userEventDetails.xhtml</to-view-id>
        </navigation-case>
     </navigation-rule>
     
     <navigation-rule>
        <from-view-id>/user/userEventDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/user/festivalsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     
     <navigation-rule>
        <from-view-id>/user/createBooking.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>transactionCreated</from-outcome>
            <to-view-id>/user/paymentSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/user/festivalsSummary.xhtml</to-view-id>
        </navigation-case>
     </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/paymentSummary.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/user/festivalsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/myTransactions.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/user/festivalsSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>publishTransaction</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteTransaction</from-outcome>
            <to-view-id>/user/confirmTransactionDeletion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>transactionDetails</from-outcome>
            <to-view-id>/user/transactionDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/confirmTransactionDeletion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>transactionDeleted</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/user/transactionDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editTransaction</from-outcome>
            <to-view-id>/user/editMyTransaction.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addTickets</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/editMyTransaction.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>transactionUpdated</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/user/myTransactions.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/eventManager/eventManagerLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginOK</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    <navigation-rule>
        <from-view-id>/eventManager/eventsSummary.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>createEvent</from-outcome>
            <to-view-id>/eventManager/createFestival.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>publishEvent</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteEvent</from-outcome>
            <to-view-id>/eventManager/confirmEventDeletion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>eventDetails</from-outcome>
            <to-view-id>/eventManager/eventDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/eventManager/eventDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editEvent</from-outcome>
            <to-view-id>/eventManager/editMyEvent.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/eventManager/editMyEvent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>eventUpdated</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/eventManager/createFestival.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>eventCreated</from-outcome>
            <to-view-id>/eventManager/eventDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/eventManager/confirmEventDeletion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>eventDeleted</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/eventManager/eventsSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/adminLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginOK</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id>/admin/venuesSummary.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createVenue</from-outcome>
            <to-view-id>/admin/createVenue.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>publishVenue</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteVenue</from-outcome>
            <to-view-id>/admin/confirmVenueDeletion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>venueDetails</from-outcome>
            <to-view-id>/admin/venueDetails.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id>/admin/createVenue.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>venueCreated</from-outcome>
            <to-view-id>/admin/venueDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/confirmVenueDeletion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>venueDeleted</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
            
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
        <from-view-id>/admin/venueDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addPerson</from-outcome>
            <to-view-id>/admin/addManager.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deletePerson</from-outcome>
            <to-view-id>/admin/venueDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editVenue</from-outcome>
            <to-view-id>/admin/editVenue.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/editVenue.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>venueUpdated</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/venuesSummary.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/addManager.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>personAdded</from-outcome>
            <to-view-id>/admin/venueDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/venueDetails.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>